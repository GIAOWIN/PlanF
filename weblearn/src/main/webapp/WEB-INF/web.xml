<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>com.yuan.util.web.servlet.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet<!--这是servlet名，和上面的一致即可 也是随便写--></servlet-name>
        <url-pattern>/myservlet<!--这个是访问路径 随便写--></url-pattern>
    </servlet-mapping>

<filter>
    <filter-name>encoding</filter-name>
    <filter-class>com.yuan.util.web.filter.EncodingFilter</filter-class>
    <init-param>
        <param-name>tomcatVersion</param-name>
        <param-value>8</param-value>
    </init-param>
</filter>
<filter-mapping>
    <filter-name>encoding</filter-name>
    <url-pattern>/*</url-pattern>
</filter-mapping>

</web-app>
